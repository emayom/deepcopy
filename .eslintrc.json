{
    "root": true,
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "no-extra-semi": "error",
        "arrow-body-style": "off",
        "prefer-arrow-callback": "off"
    }
}
